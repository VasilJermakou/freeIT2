<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>FormServlet</servlet-name>
        <servlet-class>com.vaer.hw3.servlets.FormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormServlet</servlet-name>
        <url-pattern>/forms</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CheckFormsServlet</servlet-name>
        <servlet-class>com.vaer.hw3.servlets.CheckFormsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckFormsServlet</servlet-name>
        <url-pattern>/checkforms</url-pattern>
    </servlet-mapping>

</web-app>